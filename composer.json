{
    "name": "gbere/zammad-api-client-php",
    "type": "library",
    "description": "Zammad API client for PHP",
    "homepage": "https://github.com/gbere/zammad-api-client-php",
    "keywords": ["zammad", "api"],
    "license": ["AGPL-3.0", "MIT"],
    "authors": [
        {
            "name": "Zammad GmbH",
            "homepage": "https://zammad.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/http-client": "^6.4 | ^7.3",
        "ext-mbstring": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.7 <9"
    },
    "autoload": {
        "psr-4": {"ZammadAPIClient\\": "src/"}
    }
}
